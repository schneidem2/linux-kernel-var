};
#else
&usdhc1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc1>;
	no-1-8-v;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	cd-gpios = <&gpio4 19 0>;
	status = "okay";
};
#endif

#ifdef NAND
/*#Creating 7 MTD partitions on "gpmi-nand":*/
/*#0x00000000-0x00200000 // mtd0 "spl" 			Offset: 0, 			Size = 2097152 bytes = 2MBytes,*/ 
/*#0x00200000-0x00200000 // mtd1 "uboot"		Offset: 2MBytes,	Size = 2097152 bytes = 2MBytes,*/
/*#0x00400000-0x00200000 // mtd2 "uboot-env" 	Offset: 4Mbytes,	Size = 2097152 bytes = 2MBytes, */
/*#0x00600000-0x00800000 // mtd3 "reserved1" 	Offset: 6Mbytes		Size = 2097152 bytes = 2MBytes,*/ 
/**/
/*#0x00800000-0x01000000 // mtd4 "kernel"		Offset: 8Mbytes,	Size = 8388608 bytes = 8MBytes,  */
/*#0x01000000-0x05000000 // mtd5 "rootfs"		Offset: 16Mbytes,	Size = 67108864 bytes = 64MBytes,*/ 
/**/
/*#0x05000000-0x05800000 // mtd6 "kernel"		Offset: 80Mbytes,	Size = 8388608 	bytes = 8MBytes,  */
/*#0x05800000-0x09800000 // mtd7 "rootfs"		Offset: 88Mbytes,	Size = 67108864 bytes = 64MBytes,*/ 
/**/
/*#0x09800000-0x0D800000 // mtd8 "data"			Offset: 152Mbytes,	Size = 67108864 bytes = 64MBytes, */
/*#0x0D800000-0x10000000 // mtd9 "reserved2"	Offset: 216Mbytes,	Size = 41943040 bytes = 40MBytes,*/

&gpmi {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpmi_nand_1>;
	nand-on-flash-bbt;
	status = "okay";
	
	partition@0 {
		label = "spl";
		reg = <0x00000000 0x00200000>; // Offset: 0, 		Size = 2097152 bytes = 2MBytes
	};

	partition@1 {
		label = "uboot";
		reg = <0x00200000 0x00200000>; // Offset: 2MBytes,	Size = 2097152 bytes = 2MBytes
	};

	partition@2 {
		label = "uboot-env";
		reg = <0x00400000 0x00200000>; // Offset: 4Mbytes,	Size = 2097152 bytes = 2MBytes
	};

	partition@3 {
		label = "reserved1";
		reg = <0x00600000 0x00200000>; // Offset: 6Mbytes	Size = 2097152 bytes = 2MBytes
	};
	
	partition@4 {
		label = "kernel";
		reg = <0x00800000 0x00800000>; // Offset: 8Mbytes,	Size = 8388608 bytes = 8MBytes
	};

	partition@5 {
		label = "rootfs";
		reg = <0x01000000 0x04000000>; // Offset: 16Mbytes,	Size = 67108864 bytes = 64MBytes
	};
	
	partition@6 {
		label = "kernel";
		reg = <0x05000000 0x00800000>; // Offset: 80Mbytes,	Size = 8388608 	bytes = 8MBytes
	};
	
	partition@7 {
		label = "rootfs";
		reg = <0x05800000 0x04000000>; // Offset: 88Mbytes,	Size = 67108864 bytes = 64MBytes
	};
	
	partition@8 {
		label = "data";
		reg = <0x09800000 0x04000000>; // Offset: 152Mbytes,	Size = 67108864 bytes = 64MBytes
	};
	
	partition@9 {
		label = "reserved2";
		reg = <0x0D800000 0x02800000>; // Offset: 216Mbytes,	Size = 41943040 bytes = 40MBytes
	};
};

&usdhc2 {
	status = "disabled";
};
#endif

#ifdef EMMC
&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2>;
	no-1-8-v;
	non-removable;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	status = "okay";
};
#endif

&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";

    /* DS1307 RTC module */
    rtc@0x68 {
             compatible = "dallas,ds1307";
             reg = <0x68>;
	};             
    /* MPL3115A2 Precision Altimeter */
    mpl3115@0x60 {
             compatible = "freescale,mpl3115";
             reg = <0x60>;
    };
};

&i2c2 {
	clock_frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";
#ifdef VARISCITE_AUDIO_CODEC
	codec: wm8731@1a {
		compatible = "wlf,wm8731";
		reg = <0x1a>;
		
		clocks = <&clks IMX6UL_CLK_SAI2>, <&clks IMX6UL_CLK_PLL4_AUDIO_DIV>;
		clock-names = "mclk","pll4";
	};
#endif	
#ifdef VARISCITE_TOUCH
	ft5x06_ts@38 {
			compatible = "focaltech,5x06";
			reg = <0x38>;
			focaltech,family-id = <0x54>;
			interrupt-parent = <&gpio3>;
			interrupts = <4 0>;
			vdd-supply = <&touch_3v3_regulator>;
			vcc_i2c-supply = <&touch_3v3_regulator>;
			focaltech,irq-gpio = <&gpio3 4 0x00>;
			focaltech,display-coords = <0 0 800 480>;
			focaltech,name = "ft6x06";
			focaltech,no-force-update;
			focaltech,group-id = <1>;
			focaltech,hard-reset-delay-ms = <20>;
			focaltech,soft-reset-delay-ms = <150>;
			focaltech,num-max-touches = <2>;
			focaltech,fw-name = "ft_8610_qrd_fw.bin";
			focaltech,fw-delay-aa-ms = <100>;
			focaltech,fw-delay-55-ms = <30>;
			focaltech,fw-upgrade-id1 = <0x79>;
			focaltech,fw-upgrade-id2 = <0x08>;
			focaltech,fw-delay-readid-ms = <10>;
			focaltech,fw-delay-era-flsh-ms = <2000>;
			focaltech,fw-auto-cal;
			focaltech,mirror_h;
			focaltech,mirror_v;
		};
#endif
#ifdef VARISCITE_RTC
        /* DS1307 RTC module */
        rtc@0x68 {
                 compatible = "dallas,ds1307";
                 reg = <0x68>;
        };
#endif        
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_1>;
	imx6ul-evk {
		pinctrl_hog_1: hoggrp-1 {
			fsl,pins = <
				MX6UL_PAD_CSI_HSYNC__GPIO4_IO20		0x1b0b0 /* Led 1 */
				MX6UL_PAD_CSI_DATA00__GPIO4_IO21	0x1b0b0 /* Led 2 */
				MX6UL_PAD_GPIO1_IO00__GPIO1_IO00	0x17059	/* User Button */
				MX6UL_PAD_GPIO1_IO07__ENET2_MDC		0x1b0b0
				MX6UL_PAD_GPIO1_IO06__ENET2_MDIO	0x1b0b0
				MX6UL_PAD_SNVS_TAMPER4__GPIO5_IO04	0x1b0b0	/* BT Enable */
				MX6UL_PAD_SNVS_TAMPER0__GPIO5_IO00	0x1b0b0
				MX6UL_PAD_GPIO1_IO04__GPIO1_IO04	0x80000000 /* HW_CODE2 */
				MX6UL_PAD_GPIO1_IO08__GPIO1_IO08	0x80000000 /* HW_CODE3 */
				MX6UL_PAD_GPIO1_IO09__GPIO1_IO09	0x80000000 /* HW_CODE4 */
			>;
		};

		pinctrl_enet1: enet1grp {
			fsl,pins = <
				MX6UL_PAD_ENET1_RX_EN__ENET1_RX_EN	0x1b0b0
				MX6UL_PAD_ENET1_RX_ER__ENET1_RX_ER	0x1b0b0
				MX6UL_PAD_ENET1_RX_DATA0__ENET1_RDATA00	0x1b0b0
				MX6UL_PAD_ENET1_RX_DATA1__ENET1_RDATA01	0x1b0b0
				MX6UL_PAD_ENET1_TX_EN__ENET1_TX_EN	0x1b0b0
				MX6UL_PAD_ENET1_TX_DATA0__ENET1_TDATA00	0x1b0b0
				MX6UL_PAD_ENET1_TX_DATA1__ENET1_TDATA01	0x1b0b0
				MX6UL_PAD_ENET1_TX_CLK__ENET1_REF_CLK1	0x4001b031
			>;
		};

		pinctrl_enet2: enet2grp {
			fsl,pins = <
				MX6UL_PAD_ENET2_RX_EN__ENET2_RX_EN	0x1b0b0
				MX6UL_PAD_ENET2_RX_ER__ENET2_RX_ER	0x1b0b0
				MX6UL_PAD_ENET2_RX_DATA0__ENET2_RDATA00	0x1b0b0
				MX6UL_PAD_ENET2_RX_DATA1__ENET2_RDATA01	0x1b0b0
				MX6UL_PAD_ENET2_TX_EN__ENET2_TX_EN	0x1b0b0
				MX6UL_PAD_ENET2_TX_DATA0__ENET2_TDATA00	0x1b0b0
				MX6UL_PAD_ENET2_TX_DATA1__ENET2_TDATA01	0x1b0b0
				MX6UL_PAD_ENET2_TX_CLK__ENET2_REF_CLK2	0x4001b031
				MX6UL_PAD_JTAG_MOD__GPIO1_IO10		0x1b0b0
			>;
		};

		pinctrl_flexcan1: flexcan1grp{
			fsl,pins = <
				MX6UL_PAD_UART3_RTS_B__FLEXCAN1_RX	0x1b020
				MX6UL_PAD_UART3_CTS_B__FLEXCAN1_TX	0x1b020
			>;
		};

		pinctrl_flexcan2: flexcan2grp{
			fsl,pins = <
				MX6UL_PAD_UART2_RTS_B__FLEXCAN2_RX	0x1b020
				MX6UL_PAD_UART2_CTS_B__FLEXCAN2_TX	0x1b020
			>;
		};

		pinctrl_lcdif_dat: lcdifdatgrp {
			fsl,pins = <
				MX6UL_PAD_LCD_DATA02__LCDIF_DATA02  0x79
				MX6UL_PAD_LCD_DATA03__LCDIF_DATA03  0x79
				MX6UL_PAD_LCD_DATA04__LCDIF_DATA04  0x79
				MX6UL_PAD_LCD_DATA05__LCDIF_DATA05  0x79
				MX6UL_PAD_LCD_DATA06__LCDIF_DATA06  0x79
				MX6UL_PAD_LCD_DATA07__LCDIF_DATA07  0x79
				MX6UL_PAD_LCD_DATA10__LCDIF_DATA10  0x79
				MX6UL_PAD_LCD_DATA11__LCDIF_DATA11  0x79
				MX6UL_PAD_LCD_DATA12__LCDIF_DATA12  0x79
				MX6UL_PAD_LCD_DATA13__LCDIF_DATA13  0x79
				MX6UL_PAD_LCD_DATA14__LCDIF_DATA14  0x79
				MX6UL_PAD_LCD_DATA15__LCDIF_DATA15  0x79
				MX6UL_PAD_LCD_DATA18__LCDIF_DATA18  0x79
				MX6UL_PAD_LCD_DATA19__LCDIF_DATA19  0x79
				MX6UL_PAD_LCD_DATA20__LCDIF_DATA20  0x79
				MX6UL_PAD_LCD_DATA21__LCDIF_DATA21  0x79
				MX6UL_PAD_LCD_DATA22__LCDIF_DATA22  0x79
				MX6UL_PAD_LCD_DATA23__LCDIF_DATA23  0x79
			>;
		};

		pinctrl_lcdif_ctrl: lcdifctrlgrp {
			fsl,pins = <
				MX6UL_PAD_LCD_CLK__LCDIF_CLK	    0x79
				MX6UL_PAD_LCD_ENABLE__LCDIF_ENABLE  0x79
			>;
		};

		pinctrl_pwm1: pwm1grp {
			fsl,pins = <
				MX6UL_PAD_LCD_DATA00__PWM1_OUT  0x110b0
			>;
		};

		pinctrl_uart1: uart1grp {
			fsl,pins = <
				MX6UL_PAD_UART1_TX_DATA__UART1_DCE_TX 0x1b0b1
				MX6UL_PAD_UART1_RX_DATA__UART1_DCE_RX 0x1b0b1
			>;
		};

		pinctrl_uart2: uart2grp {
			fsl,pins = <
				MX6UL_PAD_UART2_TX_DATA__UART2_DCE_TX	0x1b0b1
